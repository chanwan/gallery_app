<div class="container">
  <div class="row">
  <% @images.each do |img| %>
    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
      <a class="thumbnail" href="#">
        <%= image_tag img.picture.url(:medium), class: "img-responsive" %>
      </a>
      <div class="linkz">
        <%= link_to img.name, image_path(img) %>
        <p> By: <%= link_to img.user.name, user_path(img.user.id)  %></p>
      </div>
    </div>
    <% end %>
  </div>
</div>
